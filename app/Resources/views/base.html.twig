<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name='viewport' content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no'>
        <title>Fremartec - {% block title %}{% endblock %}</title>
        {% block stylesheets %}
 
            <link href='https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,300,300italic,400italic,600,600italic' rel='stylesheet' type='text/css'>            
            <link rel="stylesheet" href="{{ asset('bundles/fremartec/dist/bootstrap/css/bootstrap.min.css') }}">
            <link rel="stylesheet" href="{{ asset('bundles/fremartec/dist/font-awesome/css/font-awesome.min.css') }}">
            <link href='{{ asset('bundles/fremartec/dist/style/style.css') }}' rel='stylesheet' type='text/css'>
            <link href='{{ asset('bundles/fremartec/dist/animatecss/animate.css') }}' rel='stylesheet' type='text/css'>

        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body ng-app="validate" ng-controller="form">
        {% block body %}{% endblock %}

        {% block javascripts %}
            <!--
            <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>-->
            <script src="{{ asset('bundles/fremartec/dist/jquery/jquery-1.11.3.min.js') }}"></script>
            <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.16/angular.min.js"></script>
            {# controller document #}
            <script src="{{ asset('bundles/fremartec/dist/angular/document_controller.js') }}"></script>
            <!-- Latest compiled and minified JavaScript
            <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>-->
            <script src="{{ asset('bundles/fremartec/dist/bootstrap/js/bootstrap.min.js') }}"></script>
            <script src="{{ asset('bundles/fremartec/dist/lodash/lodash.js') }}"></script>
        {% endblock %}
    </body>
</html>
