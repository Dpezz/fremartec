  <link rel="stylesheet" href="{{ asset('bundles/fremartec/dist/flip/flip.css') }}">
  <link rel="stylesheet" href="{{ asset('bundles/fremartec/dist/carousel/carousel.css') }}">
  <style type="text/css">
    .navbar-default {
        background-color: #259952;
        opacity: 0.9
    }
  </style>
{% endblock %}

{% block logo %}<img src="{{ asset('bundles/fremartec/dist/images/logos/logo3.png') }}" width="120px">{% endblock %}

{% block menu %}
  <li id="menu-two"><a href="" onclick="$('#two').animatescroll();">Áreas de Servicio</a></li>
  <li id="menu-three"><a href="" onclick="$('#three').animatescroll();">Obras</a></li>
  <li id="menu-four"><a href="" onclick="$('#four').animatescroll();">Manuales</a></li>
