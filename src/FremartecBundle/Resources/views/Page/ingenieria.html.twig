			{"client":"Asoc. De Canalistas Palqui Maurat Semita","title":"Acondicionamiento y Revestimiento de estanque","address":"El Paqui, Monte Patria","description":"Acondicionamiento y limpieza de estanque de 27.000 mt3, Revestimiento con geomembrana Hdpe 1,00mm 16.825 mt2"},
			{"client":"Agricola Canta Rana S.A","title":"Revestimiento estanque","address":"Nueva Aurora, Ovalle","description":"Revestimiento geomembrana de Hdpe 1,5mm 19.405 mt2"},
			{"client":"María Gabriela Zepeda Gomila","title":"Construcción y Revestimiento de estanque","address":"Huallillinga, Ovalle","description":"Diseño y construcción estanque de 33.000 mt3, Revestimiento con geomembrana Hdpe 1,0mm 15.210 mt2"},
			{"client":"Viña Concha y Toro S.A","title":"Revestimiento estanque","address":"Los Acacios, Monte Patria","description":"Revestimiento con geomembrana Hdpe de 1,00mm 11.925 mt2"},
			{"client":"Minera HMC Gold S.M.C","title":"Pipping Planta de Flotación","address":"Punitaqui, Ovalle","description":"Montaje y termofusión pipping hdpe"},
			{"client":"Junta de Vigilancia Rio Elqui","title":"Instalación tubería hdpe Canal Santa Ines","address":"San Ramón, La Serena","description":"Montaje y termofusión tubería hdpe 700mm"},
			{"client":"Agricola Valle Verde S.A","title":"Revestimiento de estanque","address":"Recoleta, Ovalle","description":"Revestimiento con geotextil y geomembrana de Hdpe 9.620 mt2"},
			{"client":"Junta de Vigilancia Rio Limari y sus afluentes","title":"Acondicionamiento y Revestimiento de canale","address":"Las Vegas de Limari, Ovalle","description":"Acondicionamiento y limpieza de canal de 4,5 km, Revestimiento con geomembrana de Hdpe 6510 mt2"},
		] %}

		{% for item in items %}
			{% if loop.index == 1 %}
    		<div class="item active">
			{% else %}
				<div class="item">
			{% endif %}
      	<div class="fill" style="background-image:url('{{ asset('bundles/fremartec/dist/images/ing/work/image_'~(loop.index)~'.jpg') }}');"></div>
      		<div class="carousel-caption">
						<div class="row">
							<div class="col-md-6">
								<div class="panel panel-default">
									<div class="panel-body">
										<img src="{{ asset('bundles/fremartec/dist/images/ing/panel/logo.png') }}" width="50%">
										<img src="{{ asset('bundles/fremartec/dist/images/ing/panel/bg-curva.png') }}" width="100%">

											<strong>Cliente:</strong>&nbsp;{{ item.client }}.<br>
											<strong>Obra:</strong>&nbsp;{{ item.title }}.<br>
											<strong>Ubicación:</strong>&nbsp;{{ item.address }}.<br>
										  <strong>Descripción:</strong>&nbsp;{{ item.description }}.<br>

										<img src="{{ asset('bundles/fremartec/dist/images/ing/panel/hr.png') }}" width="100%">
								</div>
							</div>
						</div>
					</div>
	      </div>
    	</div>
		{% endfor %}
  </div>

  <!-- Controls -->
  <a class="left carousel-control" href="#three" data-slide="prev">
      <span class="glyphicon glyphicon-chevron-left"></span>
  </a>
  <a class="right carousel-control" href="#three" data-slide="next">
      <span class="glyphicon glyphicon-chevron-right"></span>
  </a>
</div>


