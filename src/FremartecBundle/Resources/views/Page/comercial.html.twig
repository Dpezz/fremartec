{% extends 'FremartecBundle:Layout:layout_pages.html.twig' %}

{% block title %}Comercial{% endblock %}

{% block stylesheets %}
	{{parent()}}
  <link rel="stylesheet" href="{{ asset('bundles/fremartec/dist/cover/cover.css') }}">
{% endblock %}

{% block menu %}
  <li><a href="#two">Hidraulica</a></li>
  <li><a href="#three">Bombas</a></li>
  <li><a href="#four">Riego</a></li>
{% endblock %}

{% block content %}

{# item 1 #}
<div class="site-wrapper" style="background-color:#FE939D">
    <div class="site-wrapper-inner">
        <div class="cover-container"> 

        	<div class="inner cover">
          	<h1>Comercial.</h1>
          	<p class="lead">Cover is a one-page template for building simple and beautiful home pages. Download, edit the text, and add your own fullscreen background photo to make it your own.</p>
          	<p class="lead">
            		<a href="" class="btn btn-lg btn-default"><i class="fa fa-heart"></i>&nbsp;Sign In</a>
          	</p>
        	</div>
          
        </div>
    </div>
</div>

{# item 2 #}
<div id="two" class="site-wrapper" style="background-color:#fff">
  <div class="site-wrapper-inner">
    <div class="cover-container"> 

      <div class="inner cover">

        <h1 class="page-header text-left text-info">Title number Two</h1>
        <div class="row">
          <div class="col-md-4">
            <img class="img-circle" width="80%" src="data:image/svg+xml;base64,">
            <br>
            <br>
            <a href="{{ path('comercial') }}" class="badge">Comercial</a>
          </div>
          <div class="col-md-4">
            <img class="img-circle" width="80%" src="data:image/svg+xml;base64,">
            <br>
            <br>
            <a href="{{ path('ingenieria') }}" class="badge">Ingenieria</a>
          </div>
          <div class="col-md-4">
            <img class="img-circle" width="80%" src="data:image/svg+xml;base64,">
            <br>
            <br>
            <a href="{{ path('consultora') }}" class="badge">Consultora</a>
          </div>
        </div>
      </div>
          
    </div>
  </div>
</div>

{# item 3 #}
<div id="three" class="site-wrapper" style="background-color:#fff">
  <div class="site-wrapper-inner">
    <div class="cover-container"> 

      <div class="inner cover">
        <h1 class="page-header text-right text-info">Title number Three</h1>
        <div class="row">
          <div class="col-md-5">
            <img class="img-rounded" width="90%" src="data:image/svg+xml;base64,">
          </div>
          <div class="col-md-7">
            <h1 class="text-info">Lorem ipsum dolor sit amet,<small>consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.</small></h1>  
          </div>
        </div>
      </div>

    </div>
  </div>
</div>

<div ng-app="validate" ng-controller="form">
{# item 4 #}
<div id="four" class="site-wrapper" style="background-color:#fff" >
  <div class="site-wrapper-inner">
    <div class="cover-container"> 

      <div class="inner cover">
        <h1 class="page-header text-left text-info">Title number Four</h1>
        <div class="row">
          <div class="col-md-5">
            <img class="img-rounded" width="90%" src="data:image/svg+xml;base64,">
          </div>
          <div class="col-md-7">
          {% set url = path(app.request.attributes.get('_route'))|trim('/') %}
            <h1 class="text-info">Lorem ipsum dolor sit amet,<small>consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.</small></h1>
            <a href="" type="button" class="btn btn-primary" data-toggle="modal" data-target="#download" title="download" ng-click="download('primero','comercial')" ><i class="fa fa-download"></i>&nbsp;Descargar</a>
            <a href="" type="button" class="btn btn-primary" data-toggle="modal" data-target="#download" title="download" ng-click="download('primero','comercial')" ><i class="fa fa-download"></i>&nbsp;Descargar</a>


          </div>
        </div>
      </div>

    </div>
  </div>
</div>


{# footer #}
<div class="container">
  <hr>
  <footer>
    <h6 class="text-center text-muted">
      (+569) 81791226  -   contacto@pagescolar.cl
      Pedro Pablo Muñoz #250, La Serena, Chile - 2015</h6>
  </footer>
</div>



<!-- Modal -->
<div class="modal fade" id="download" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title text-left text-muted" id="myModalLabel"><i class="fa fa-download"></i>&nbsp;Descargar</h4>
          </div>
          <div class="modal-body">
            <form name="userForm" novalidate>
              <div class="form-group">
                <h5 class="text-left text-muted"><strong>Ingresa tu Correo Electrónico</strong></h5>
                <input type="email" class="form-control" ng-model="email" name="email" placeholder="Email" required autofocus></input>
              </div>
              <div class="alert alert-success" role="alert" ng-show="visible">
                <strong><i class="fa fa-check-circle"></i>&emsp;Gracias</strong> por preferir Fremartec.
              </div>
              <div class="modal-footer">
                <a type="button" class="btn btn-info" data-dismiss="modal"><i class="fa fa-remove"></i>&nbsp;Cerrar</a>
                <button class="btn btn-primary" ng-disabled="userForm.$invalid" ng-click="download_save()"><i class="fa fa-download" data-dismiss="modal"></i>&nbsp;Descargar</button>
              </div>
            </form>
        </div>
      </div>
  </div>
</div>

</div>
{% endblock %}
